LDFLAGS += --sysroot ${HOME}/opt/opendingux-toolchain -L${HOME}/opt/opendingux-toolchain/lib
LDLIBS += -lSDL -lasound -lpng -lz -lm -ldl

ARCH = mipsel
PLATFORM = opendingux
SOUND_DRIVERS = sdl
